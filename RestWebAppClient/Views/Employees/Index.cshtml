@using RestWebAppClient.Helpers;
@model IEnumerable<RestWebAppClient.Models.Employee>

@{
    ViewBag.Title = "Employees";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Prijmeni)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jmeno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.D_Narozeni)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vyska)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Prijmeni)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jmeno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.D_Narozeni)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vyska)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
